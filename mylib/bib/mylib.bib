@article{krepelMAPPINGSODIUMVOID2021,
  title = {{{MAPPING OF SODIUM VOID EFFECT AND DOPPLER CONSTANT IN ESFR-SMART CORE WITH MONTE CARLO CODE SERPENT AND DETERMINISTIC CODE ERANOS}}},
  author = {Krepel, Jiri and Raffuzzi, Valeria},
